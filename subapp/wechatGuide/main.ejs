            threshold : 200,
            skip_invisible : false
        });
    },1500);

    var _$wcg_logo = $(".wcg-logo");
    $(".wcg-cate").delegate("li", "click", function(e) {
        e.preventDefault();
        var _$this = $(this),
            _ref = _$this.data("ref");
        $.ajax({
            url:"/wcg/logo",
            type: "get",
            data:{
                ref:_ref
            }

        }).success(function(data){
            console.log(data);
            _$wcg_logo.html(data);        
