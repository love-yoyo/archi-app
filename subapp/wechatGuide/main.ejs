                _left = _a.offset().left,
                _top = _a.offset().top,
                _itemWidth = _a.width();

            console.log("left:"+_left+" top:"+_top+" itemWidth:"+_itemWidth+" container:"+_containerWidth+" height:"+_containerHeight);
            if (_find.length>0) {
                _find.addClass("active");
                console.log("der:"+parseFloat(_containerWidth-_left-_itemWidth));
                var itemLeft = parseFloat(_containerWidth-_left-_itemWidth)<350 ? -360 : 100;
                var itemTop = 0;
                _find.css({
                    left: itemLeft,
                    top: itemTop
                })
                
            } else {
            }
        },500);
    }).mouseout(function(e){
        var _this = $(this);
